{
  "testRegex": ".test\\.js$",
  "roots": [
    "packages/"
  ],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
  },
  "collectCoverageFrom": [
    "packages/**/*.js",
    "!packages/**/*.stories.js",
    "!packages/**/index.js"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/jest.config.js",
  "moduleNameMapper": {
    "\\.(css|less|scss)$": "identity-obj-proxy"
  }
}
