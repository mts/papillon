{
  "name": "papillon-alerts",
  "version": "1.0.41",
  "description": "Papillon Alerts React components for Primer Alerts",
  "homepage": "https://github.com/mts/papillon",
  "repository": "https://github.com/mts/papillon/tree/master/packages/papillon-alerts",
  "main": "build/papillon-alerts.js",
  "author": "MTS",
  "license": "MIT",
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "alerts",
    "papillon",
    "primer",
    "css",
    "react",
    "github",
    "design-system",
    "component-library"
  ],
  "scripts": {
    "build": "webpack --env production",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm i",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "uninstall-primer": "npm un -S primer",
    "install-primer": "npm i -S primer",
    "reinstall-primer": "npm-run-all uninstall-primer install-primer",
    "test": "jest --config jest.config.json --no-cache --silent --runInBand",
    "lint": "npm-run-all lint:scss lint:js",
    "lint:scss": "stylelint lib/*.scss --fix",
    "lint:js": "eslint lib/*.js --fix",
    "lint:staged": "lint-staged",
    "ci": "npm-run-all lint build test"
  },
  "dependencies": {
    "primer": "10.10.2"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "jest": "23.6.0"
  }
}
