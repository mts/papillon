{
  "name": "papillon-alerts",
  "version": "1.0.40",
  "description": "Papillon Alerts React components for Primer Alerts",
  "homepage": "https://github.com/mts/papillon",
  "repository": "https://github.com/mts/papillon/tree/master/packages/papillon-alerts",
  "main": "build/papillon-alerts.js",
  "author": "MTS",
  "license": "MIT",
  "files": [
    "lib",
    "build",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "alerts",
    "papillon",
    "primer",
    "css",
    "react",
    "github",
    "design-system",
    "component-library"
  ],
  "scripts": {
    "build": "webpack --env production",
    "remove-packages": "rm -rf node_modules package-lock.json",
    "add-packages": "npm i",
    "update-packages": "npm-run-all remove-packages add-packages",
    "uninstall-primer": "npm un -S primer",
    "install-primer": "npm i -S primer",
    "update-primer": "npm-run-all uninstall-primer install-primer",
    "release": "npm publish",
    "test": "jest --config jest.config.json --no-cache --silent --runInBand",
    "lint": "npm-run-all lint:scss lint:js",
    "lint:scss": "stylelint lib/*.scss --fix",
    "lint:js": "eslint lib/*.js --fix",
    "lint:staged": "lint-staged",
    "ci": "npm-run-all lint test build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": [
    "lint:staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "primer": "10.10.2",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-test-renderer": "16.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "1.0.0",
    "cssnano": "4.1.7",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "git-revision-webpack-plugin": "3.0.3",
    "glob": "7.1.3",
    "husky": "1.2.0",
    "jest": "23.6.0",
    "jest-enzyme": "7.0.1",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.4.5",
    "nodemon": "1.18.7",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-cssnext": "3.1.0",
    "prettier": "1.15.3",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "react-hot-loader": "4.3.12",
    "stylelint": "9.9.0",
    "stylelint-order": "2.0.0",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.27.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4",
    "yeticss": "7.4.1"
  }
}
