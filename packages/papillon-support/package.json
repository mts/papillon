{
  "name": "papillon-support",
  "version": "1.0.49",
  "description": "Papillon Support react components for Primer Support",
  "homepage": "https://github.com/mts/papillon",
  "repository": "https://github.com/mts/papillon/tree/master/packages/papillon-support",
  "main": "lib/papillon-support.js",
  "license": "MIT",
  "author": "MTS",
  "keywords": [
    "support",
    "papillon",
    "primer",
    "css",
    "react",
    "github",
    "design-system",
    "component-library"
  ],
  "scripts": {
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm i",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "npm-update": "npm update",
    "uninstall-primer": "npm un -S @primer/css",
    "install-primer": "npm i -S @primer/css",
    "reinstall-primer": "npm-run-all uninstall-primer install-primer"
  },
  "dependencies": {
    "@primer/css": "^12.6.0"
  }
}
