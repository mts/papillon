{
  "name": "papillon-breadcrumb",
  "version": "1.0.47",
  "description": "Papillon Breadcrumb React components for Primer Breadcrumb",
  "homepage": "https://github.com/mts/papillon",
  "repository": "https://github.com/mts/papillon/tree/master/packages/papillon-breadcrumb",
  "main": "build/papillon-breadcrumb.js",
  "author": "MTS",
  "license": "MIT",
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "breadcrumb",
    "papillon",
    "primer",
    "css",
    "react",
    "github",
    "design-system",
    "component-library"
  ],
  "scripts": {
    "build": "webpack --env production",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm i",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "npm-update": "npm update",
    "uninstall-primer": "npm un -S @primer/css",
    "install-primer": "npm i -S @primer/css",
    "reinstall-primer": "npm-run-all uninstall-primer install-primer",
    "test": "jest -u --config jest.config.json --no-cache --silent --runInBand",
    "lint": "npm-run-all lint:scss lint:js",
    "lint:scss": "stylelint lib/*.scss --fix",
    "lint:js": "eslint lib/*.js --fix",
    "lint:staged": "lint-staged",
    "ci": "npm-run-all lint build test"
  },
  "dependencies": {
    "@primer/css": "^12.1.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "jest": "^24.3.1"
  }
}
