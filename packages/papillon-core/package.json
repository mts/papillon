{
  "name": "papillon-core",
  "version": "1.0.52",
  "description": "Papillon Core React components for Primer Core",
  "homepage": "https://github.com/mts/papillon",
  "repository": "https://github.com/mts/papillon/tree/master/packages/papillon-core",
  "main": "build/papillon-core.js",
  "author": "MTS",
  "license": "MIT",
  "keywords": [
    "core",
    "papillon",
    "primer",
    "css",
    "react",
    "github",
    "design-system",
    "component-library"
  ],
  "scripts": {
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm i",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "npm-update": "npm update",
    "uninstall-primer": "npm un -S @primer/css",
    "install-primer": "npm i -S @primer/css",
    "reinstall-primer": "npm-run-all uninstall-primer install-primer"
  },
  "dependencies": {
    "@primer/css": "^13.2.0"
  }
}
